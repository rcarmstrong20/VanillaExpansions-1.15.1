{
    "multipart":
	[
        {   "when": { "up": "true" },
            "apply": { "model": "ve:block/stripped_acacia_wall_post" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "ve:block/stripped_acacia_wall_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "ve:block/stripped_acacia_wall_side", "y": 90, "uvlock": true }

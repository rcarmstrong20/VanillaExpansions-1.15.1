        {   "when": { "south": "true" },
            "apply": { "model": "ve:block/stripped_acacia_wall_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "ve:block/stripped_acacia_wall_side", "y": 270, "uvlock": true }
        }
    ]
